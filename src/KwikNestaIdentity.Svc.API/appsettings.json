{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Information",
        "System": "Warning"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://localhost:10000",
        "Protocols": "Http2"
      },
      "GrpcSecure": {
        "Url": "https://localhost:5001",
        "Protocols": "Http2"
      }
    }
  },
  "AllowedHosts": "*"
}
